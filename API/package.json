{
  "name": "orders-api",
  "version": "1.0.0",
  "description": "Just Help a Friend",
  "keywords": [
    "API",
    "TEST",
    "MS"
  ],
  "homepage": "https://github.com/Victor-Palha/Orders-API#readme",
  "bugs": {
    "url": "https://github.com/Victor-Palha/Orders-API/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Victor-Palha/Orders-API.git"
  },
  "license": "MIT",
  "author": "Ash",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "pnpm migrate:latest && node dist/main",
    "test": "vitest run",
    "lint": "biome check --write",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "test:unit": "vitest run --config ./vitest.config.ts",
    "migrate:make": "knex migrate:make --knexfile knexfile.js -x ts",
    "migrate:latest": "knex migrate:latest --knexfile knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile knexfile.js",
    "migrate:status": "knex migrate:status --knexfile knexfile.js"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.972.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@prisma/client": "^6.18.0",
    "@scalar/nestjs-api-reference": "^1.0.5",
    "@swc/cli": "^0.7.8",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "knex": "^3.1.0",
    "mysql2": "^3.16.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@faker-js/faker": "^10.0.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/core": "^1.13.5",
    "@types/express": "^5.0.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "globals": "^16.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "unplugin-swc": "^1.5.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
